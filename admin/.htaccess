RewriteEngine on
RewriteRule ^error$ error.html [L]
RewriteRule ^login$ index.php [L]
RewriteRule ^dashboard$ dashboard.php [L]
RewriteRule ^tender$ dashboard.php?menu=tender [L]
RewriteRule ^non_tender$ dashboard.php?menu=non_tender [L]
RewriteRule ^add-tender$ dashboard.php?menu=tambah_tender [L]
RewriteRule ^edit-tender-(.*)$ dashboard.php?menu=edit_tender&id=$1 [L]
RewriteRule ^add-sppbj-(.*)$ dashboard.php?menu=sppbj&id=$1 [L]
RewriteRule ^tambah-sppbj-(.*)$ dashboard.php?menu=tambah_sppbj&id=$1 [L]
RewriteRule ^add-non-tender$ dashboard.php?menu=tambah_non_tender [L]
RewriteRule ^edit-non-tender-(.*)$ dashboard.php?menu=edit_non_tender&id=$1 [L]
RewriteRule ^sppbj-non-tender-(.*)$ dashboard.php?menu=sppbj_non_tender&id=$1 [L]
RewriteRule ^tambah-sppbj-non-tender-(.*)$ dashboard.php?menu=tambah_sppbj_non_tender&id=$1 [L]

Options All Indexes
ErrorDocument 404 error